<?xml version='1.0'?>
<sdf version="1.6">
  <model name="drone_with_gimbal_camera">
    <include>
      <uri>model://iris_base</uri>
    </include>

    <!-- Gimbal Yaw Joint -->
    <link name="gimbal_yaw_link">
      <pose>0 0 -0.02 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <iyy>0.0001</iyy>
          <izz>0.0001</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="gimbal_yaw_joint" type="revolute">
      <parent>iris::base_link</parent>
      <child>gimbal_yaw_link</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower>
          <upper>1.57</upper>
        </limit>
      </axis>
    </joint>

    <!-- Gimbal Pitch Joint -->
    <link name="gimbal_pitch_link">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.0001</ixx>
          <iyy>0.0001</iyy>
          <izz>0.0001</izz>
        </inertia>
      </inertial>
    </link>
    <joint name="gimbal_pitch_joint" type="revolute">
      <parent>gimbal_yaw_link</parent>
      <child>gimbal_pitch_link</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.785</lower>
          <upper>0.785</upper>
        </limit>
      </axis>
    </joint>

    <!-- Camera Link -->
    <link name="cam_link">
      <pose>0 0 0.05 0 0 0</pose>
      <inertial>
        <mass>0.01</mass>
        <inertia>
          <ixx>0.001</ixx>
          <iyy>0.001</iyy>
          <izz>0.001</izz>
        </inertia>
      </inertial>
      <visual name="cam_visual">
        <pose>0 0 0 0 1.5707 0</pose>
        <geometry>
          <cylinder>
            <radius>0.005</radius>
            <length>0.018</length>
          </cylinder>
        </geometry>
      </visual>
      <sensor name="cam" type="camera">
        <pose>0 0 0 0 0.75 0</pose>
        <camera>
          <horizontal_fov>1.2</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
          </image>
          <clip>
            <near>0.1</near>
            <far>1000</far>
          </clip>
        </camera>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
          <always_on>true</always_on>
          <update_rate>30</update_rate>
          <cameraName>webcam</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>camera_link</frameName>
        </plugin>
      </sensor>
    </link>
    <joint name="camera_mount" type="fixed">
      <parent>gimbal_pitch_link</parent>
      <child>cam_link</child>
    </joint>
 

    <!-- plugins -->
    <plugin name="rotor_0_blade_1" filename="libLiftDragPlugin.so">
      <a0>0.3</a0>
      <alpha_stall>1.4</alpha_stall>
      <cla>4.2500</cla>
      <cda>0.10</cda>
      <cma>0.00</cma>
      <cla_stall>-0.025</cla_stall>
      <cda_stall>0.0</cda_stall>
      <cma_stall>0.0</cma_stall>
      <area>0.002</area>
      <air_density>1.2041</air_density>
      <cp>0.084 0 0</cp>
      <forward>0 1 0</forward>
      <upward>0 0 1</upward>
      <link_name>iris::rotor_0</link_name>
    </plugin>
